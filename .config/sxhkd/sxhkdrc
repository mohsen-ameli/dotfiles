super + d
  rofi -show drun
super + w
  dmenu-wallpaper
super + e
  thunar
super + b
  brave
super + s
  dmenu-screenshot
super + c
  rofi -show calc -no-show-match -no-sort
super + v
  clipcat-menu
Print
  screenshot --portion
alt + 1
  dmenu-power
alt + {2,3,4,5,6}
  dmenu-power {lock,shutdown,restart,logout,suspend}
shift + print
  scrot

