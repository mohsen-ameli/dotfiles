super + d
  dmenu_run
super + w
  dmenu-wallpaper
super + p
  passmenu
super + e
  thunar
super + b
  xdg-settings get default-web-browser | xargs gtk-launch
super + s
  dmenu-screenshot
super + c
  rofi -show calc -no-show-match -no-sort
super + v
  clipcat-menu
Print
  screenshot --portion
super + Print
  screenshot --full
alt + 1
  dmenu-power
alt + {2,3,4,5,6}
  dmenu-power {lock,shutdown,restart,logout,suspend}
super + space
  keyboard --next

